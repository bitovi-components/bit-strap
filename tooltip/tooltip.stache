{{#if visible}}
<div id="{{tooltipId}}" class="tooltip {{placement}} fade in" role="tooltip" {{positionTooltip}}>
    <div class="tooltip-arrow"></div>
    <div class="tooltip-inner">
        {{tooltip}}
    </div>
</div>
{{/if}}
<div class="tooltip-trigger" {{#visible}}aria-describedby="{{tooltipId}}"{{/visible}}>
    <content></content>
</div>
